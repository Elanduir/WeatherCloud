<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
        PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
    <servlet>
        <servlet-name>servlet.WeatherCurrent</servlet-name>
        <servlet-class>servlet.WeatherCurrent</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>servlet.WeatherOverview</servlet-name>
        <servlet-class>servlet.WeatherOverview</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>servlet.WeatherCache</servlet-name>
        <servlet-class>servlet.WeatherCache</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>servlet.DefaultResponse</servlet-name>
        <servlet-class>servlet.DefaultResponse</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>servlet.WeatherCurrent</servlet-name>
        <url-pattern>/weatherCurrent/*</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>servlet.WeatherOverview</servlet-name>
        <url-pattern>/weatherOverview/*</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>servlet.WeatherCache</servlet-name>
        <url-pattern>/weatherCache/*</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>servlet.DefaultResponse</servlet-name>
        <url-pattern>/*</url-pattern>
    </servlet-mapping>

</web-app>